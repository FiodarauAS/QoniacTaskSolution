<Application x:Class="QoniacTask.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:QoniacTask.WPF" 
             xmlns:views="clr-namespace:QoniacTask.WPF.Views"
             xmlns:viewmodels="clr-namespace:QoniacTask.WPF.ViewModels">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
            </ResourceDictionary.MergedDictionaries>

            <DataTemplate DataType="{x:Type viewmodels:ConverterViewModel}">
                <views:ConverterView />
            </DataTemplate>
        </ResourceDictionary>

    </Application.Resources>
</Application>
